#include <iostream>
#include <string>
using namespace std;

int main() {
    string str;
    cin >> str;
    cout << str << endl;
    for (int i = 1; i < str.size() - 1; i++) {
        for (int j = 0; j < str.size(); j++) {
            if (j < 3) {
                cout << (char)(str[j] + 3);
            } else (j >= 3 && j < str.size()) {
                cout << (char)(str[j] - 3);
            }
        }
        cout << endl;
    }

    return 0;
}

